/**
* Author: Jack Robbins
* Test our new ability to deal with hexadecmial constants
*/

pub fn byte_hex() -> i8 {
	ret 0x33y;
}

pub fn ubyte_hex() -> u8 {
	ret 0x33UY;
}

pub fn short_hex() -> i16 {
	ret 0x33s;
}

pub fn ushort_hex() -> u16 {
	ret 0x33us;
}

pub fn int_hex() -> i32 {
	ret 0x33;
}

pub fn uint_hex() -> u32 {
	ret 0x33u;
}

pub fn long_hex() -> i64 {
	ret 0x33l;
}

pub fn ulong_hex() -> u64 {
	ret 0x33ul;
}

pub fn main() -> i32 {
	ret 0;
}
